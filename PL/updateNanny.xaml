        <xctk:TimePicker x:Name="MonEnd" Format="ShortTime" Value="00:00" Margin="125,95,94,10" Grid.Row="3" 
                         IsEnabled="{Binding IsChecked, ElementName=MonCheck}" Grid.Column="1"/>
        <xctk:TimePicker x:Name="MonStart" Format="ShortTime"   Value="00:00"  Margin="125,61,94,45" Grid.Row="3" 
