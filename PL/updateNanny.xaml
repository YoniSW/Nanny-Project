        <Label x:Name="fri" Content="FRI" HorizontalAlignment="Left" Margin="360,11,0,0" VerticalAlignment="Top" Height="21" FontSize="10" RenderTransformOrigin="0.313,0.605" Grid.Row="3" Grid.Column="1" Width="47"/>
