        <ComboBox x:Name="IdNanny" HorizontalAlignment="Left" 
                  Margin="41,14,0,0" VerticalAlignment="Top" Width="172"  
                  DisplayMemberPath="_nannyID" SelectionChanged="IdNanny_SelectionChanged" Grid.Row="1" Grid.Column="1"/>
    </Grid>
