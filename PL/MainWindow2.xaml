<Window x:Class="PL.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="Window1" Height="678" Width="980">
    <Grid Margin="38,0,0,0">
        <Image x:Name="NannyBackgrownd"  Source="images/NannyBackgrownd2.jpg" Stretch="Fill" Margin="-40,-10,0,0"/>

        <Button Margin="557,142,0,0" Background="Transparent" BorderBrush="Transparent" Click="click_addNanny" HorizontalAlignment="Left" Width="299" Height="160" VerticalAlignment="Top">
            <StackPanel HorizontalAlignment="Center">
                <Image x:Name="NannyButton" Source="images/NannyButton2.png" Stretch="Fill" Width="307" Height="161"/>
            </StackPanel>
        </Button>

        <Button Margin="111,142,0,0" Background="Transparent" BorderBrush="Transparent" HorizontalAlignment="Left" Width="299" Height="127" VerticalAlignment="Top">
            <StackPanel HorizontalAlignment="Center">
                <Image x:Name="MotherButton" Source="images/mother button.png" Stretch="Fill" Width="315" Height="100"/>
            </StackPanel>
        </Button>



    </Grid>
</Window>
